<h1>Eat Da Burger!</h1>

<h2>Ready to eat!</h2>

<ul>
  {{#each burgers}}
    {{#unless devoured}}
      <li>{{ this }}</li>
    {{/unless}}
  {{/each}}
</ul>

<h2>Devoured Burgers!</h2>

<ul>
  {{#each burgers}}
    {{#if devoured}}
    <li>{{ this }}</li>
    {{/if}}
  {{/each}}
</ul>





<h2>Add a burger!</h2>
<div id="container">







    <form>



    </form>





</div>







